{
  "name": "ChatApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "server": "node chat-server.js",
    "server:dev": "nodemon chat-server.js",
    "test": "jest",
    "e2e:build:consumer": "detox build -c ios.sim.consumer.debug",
    "e2e:build:provider": "detox build -c ios.sim.provider.debug",
    "e2e:build": "yarn e2e:build:consumer && yarn e2e:build:provider",
    "e2e": "./e2e/crossdevice-detox/parallel_commands.sh \"yarn e2e:consumer\" \"yarn e2e:provider\"",
    "e2e:reuse": "./e2e/crossdevice-detox/parallel_commands.sh \"yarn e2e:consumer -- --reuse\" \"yarn e2e:provider -- --reuse\"",
    "e2e:consumer": "detox test -c ios.sim.consumer.debug",
    "e2e:provider": "detox test -c ios.sim.provider.debug"
  },
  "dependencies": {
    "@expo/react-native-touchable-native-feedback-safe": "^1.1.2",
    "express": "^4.16.2",
    "react": "16.0.0",
    "react-native": "0.50.4",
    "react-native-gifted-chat-flat": "0.8.3",
    "react-native-vector-icons": "^4.4.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-native": "4.0.0",
    "detox": "^6.0.2",
    "jest": "21.2.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.5",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.consumer.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ChatApp.app",
        "build": "xcodebuild -project ios/ChatApp.xcodeproj -scheme ChatApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "ios.sim.provider.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ChatApp.app",
        "build": "xcodebuild -project ios/ChatApp.xcodeproj -scheme ChatApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8"
      }
    }
  }
}
